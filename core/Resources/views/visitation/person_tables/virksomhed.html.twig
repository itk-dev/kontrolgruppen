<div class="accordion mb-3" id="tableAccordionVirksomhed">
    <div class="card">
        <div class="card-header text-black" id="tableHeadingVirksomhed">
            <h2 class="mb-0">
                <button class="btn btn-block text-left text-black collapsed btn-header " type="button" data-toggle="collapse" data-target="#tableCollapseVirksomhed" aria-expanded="false" aria-controls="tableCollapseVirksomhed">
                    Virksomhed <i class="fa fa-chevron-down float-right" aria-hidden="true"></i>
                </button>
            </h2>
        </div>
        <div id="tableCollapseVirksomhed" class="collapse" aria-labelledby="tableHeadingVirksomhed" data-parent="#tableAccordionVirksomhed">
            <div class="card-body p-0">
                <table class="table mb-0 table-bordered bg-info">
                    <thead class="data-header">
                        <tr>
                            <th>CVR</th>
                            <th>Relation</th>
                        </tr>
                    </thead>
                    <tbody class="bg-light">
                        {% if data.Person.Statsborgerskab is defined %}
                            {% for land in data.Person.Statsborgerskab %}
                            <tr>
                                <td>{{ land.cprlandenavn }}</td>
                                {% if land.virkningTil is defined %}
                                    <td>{{ land.virkningFra|date("d-m-Y") }} til {{ land.virkningTil|date("d-m-Y") }} </td>
                                {% else %}
                                    <td>{{ land.virkningFra|date("d-m-Y") }}</td>
                                {% endif %}
                            </tr>
                            {% endfor %}
                        {% else %}
                            <tr>
                                <td class="empty-column" colspan="4">{{ "visitation.data.empty" | trans}}</td>
                            </tr>
                        {% endif %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>