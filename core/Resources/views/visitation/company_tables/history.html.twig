<div class="accordion mb-3" id="tableAccordionHistory">
    <div class="card">
        <div class="card-header text-black" id="tableHeadingHistory">
            <h2 class="mb-0">
                <button class="btn btn-block text-left text-black collapsed btn-header " type="button" data-toggle="collapse" data-target="#tableCollapseHistory" aria-expanded="false" aria-controls="tableCollapseHistory">
                    {{ 'visitation.company.history'|trans }} <i class="iconClass fas fa-sort-down"></i>
                </button>
            </h2>
        </div>
        <div id="tableCollapseHistory" class="collapse" aria-labelledby="tableHeadingHistory" data-parent="#tableAccordionHistory">
            <div class="card-body p-0">
                <table class="table mb-0 table-bordered bg-info">
                    <thead class="data-header">
                    </thead>
                    <tbody class="bg-light">
                        {# Loop through the data for this table if applicable. Otherwise, statically add rows. #}
                        <tr>
                            <td class="empty-column" colspan="2">{{ "visitation.data.empty"|trans }}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
