<div class="accordion mb-3" id="tableAccordionCreatedDate">
    <div class="card">
        <div class="card-header text-black" id="tableHeadingCreatedDate">
            <h2 class="mb-0">
                <button class="btn btn-block text-left text-black collapsed btn-header " type="button" data-toggle="collapse" data-target="#tableCollapseCreatedDate" aria-expanded="false" aria-controls="tableCollapseCreatedDate">
                    {{ 'visitation.company.creation_date'|trans }} <i class="iconClass fas fa-sort-down"></i>
                </button>
            </h2>
        </div>
        <div id="tableCollapseCreatedDate" class="collapse" aria-labelledby="tableHeadingCreatedDate" data-parent="#tableAccordionCreatedDate">
            <div class="card-body p-0">
                <table class="table mb-0 table-bordered bg-info">
                    <thead class="data-header">
                        <tr>
                            <th>{{ 'visitation.company.date'|trans }}</th>
                        </tr>
                    </thead>
                    <tbody class="bg-light">
                        {# Loop through the data for this table if applicable. Otherwise, statically add rows. #}
                        <tr>
                            <td>{{ data.virksomhed.virksomhedStartdato|date("d-m-Y") }}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
